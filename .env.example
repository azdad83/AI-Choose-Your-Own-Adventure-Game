# AI Choose Your Own Adventure Game Configuration
# Copy this file to .env and fill in your values

# AI Provider Configuration
# Options: "ollama" or "openai"
AI_PROVIDER=openai

# Embedding Provider Configuration (can be different from AI_PROVIDER)
# Options: "ollama" or "openai"
EMBEDDING_PROVIDER=ollama

# OpenAI Configuration (only needed if AI_PROVIDER=openai or EMBEDDING_PROVIDER=openai)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# Ollama Configuration (only needed if AI_PROVIDER=ollama or EMBEDDING_PROVIDER=ollama)
OLLAMA_MODEL=gemma3:12b
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_EMBEDDING_MODEL=nomic-embed-text

# Qdrant Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333

# General Configuration
ENVIRONMENT=development
